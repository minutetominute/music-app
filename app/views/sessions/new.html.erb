<h1>Please log in!</h1>

<form action="<%= session_url %>" method="post">
  <%= csrf_html %>
  <table>
    <tr>
      <td>
        <label for="new_session_email">Email</label>
      </td>
      <td>
        <input type="text" name="email">
      </td>
    </tr>

    <br>

    <tr>
      <td>
        <label for="new_session_password">Password</label>
      </td>
      <td>
        <input type="password" name="password">
      </td>
    </tr>
  </table>
  <input type="submit" value="Log in!">
</form>
